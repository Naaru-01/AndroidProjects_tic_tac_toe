<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tictactoeapp">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:configChanges="orientation|screenSize"
        android:icon="@mipmap/tic"
        android:label="TicTacToe"

        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <!-- INSIDE ACTIVITY: android:label="name of the lable  want to see on the respective activity name " -->
        <activity
            android:name=".CodeActivity"
            android:configChanges="orientation|screenSize|smallestScreenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ThirdPage"
            android:configChanges="orientation|screenSize|smallestScreenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SecondPage"
            android:configChanges="orientation|screenSize|smallestScreenSize|keyboardHidden"
           android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize|smallestScreenSize|keyboardHidden"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Firstpage"
            android:configChanges="orientation|screenSize|smallestScreenSize|keyboardHidden"
           android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>